{
  "name": "prompt-builder",
  "version": "0.1.0",
  "description": "A toolkit for building, managing, and deploying dynamic prompts within the OpenClaw ecosystem",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "bun run build:client && bun run build:server",
    "build:client": "bun vite build --mode client",
    "build:server": "bun vite build --mode server",
    "start": "bun dist/server/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "type-check": "tsc --noEmit",
    "convex": "convex",
    "dev:frontend": "vite",
    "dev:backend": "convex dev"
  },
  "keywords": [
    "prompt-engineering",
    "ai",
    "openclaw",
    "react",
    "typescript",
    "convex"
  ],
  "author": "Andy Solomon",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@tanstack/react-query": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.0.0",
    "lucide-react": "^0.563.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^5.1.3",
    "autoprefixer": "^10.4.0",
    "esbuild": "0.25.9",
    "eslint": "^8.50.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "vite": "^7.3.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "packageManager": "bun@1.0.0"
}
